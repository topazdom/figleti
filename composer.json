{
  "name": "topazdom/figleti",
  "type": "library",
  "description": "Figlet text generator - PHP",
  "keywords": ["github", "figlet", "ascii", "text", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Povilas Susinskas",
      "email": "povilassusinskas@gmail.com"
    },
    {
      "name": "Topazdom Technologies Ltd.",
      "email": "contact@topazdom.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^6.5 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Topazdom\\Figlet\\": "src/Figlet"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Topazdom\\Figlet\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
